{
  "require": {
    "silex/silex": "~1.2",
    "guzzlehttp/guzzle": "*",
    "symfony/yaml":"~2.6",
    "symfony/config":"~2.6",
    "symfony/security":"~2.6",
    "deralex/yaml-config-service-provider": "1.0.1",
    "drupalci/results-api": "dev-master",
    "monolog/monolog": "~1.13"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.4",
    "symfony/browser-kit": "2.*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin/"
  },
  "autoload": {
    "psr-0": {
      "API\\": "src/"
    },
    "psr-4": {
      "API\\Tests\\": "tests/"
    }
  }
}
