{
  "require": {
    "silex/silex": "~1.2",
    "guzzlehttp/guzzle": "*",
    "symfony/yaml":"~2.6",
    "symfony/config":"~2.6",
    "symfony/security":"~2.6",
    "deralex/yaml-config-service-provider": "1.0.1",
    "drupalci/results-api": "dev-master",
    "symfony/http-foundation": "2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.4.*@dev",
    "satooshi/php-coveralls": "v0.6.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin/"
  },
  "autoload": {
    "psr-0": {
      "API\\": "src/"
    },
    "psr-4": {
      "API\\Tests\\": "tests/"
    }
  }
}
